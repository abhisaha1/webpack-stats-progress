{
  "name": "webpack-stats-progress",
  "version": "0.0.2",
  "description": "A webpack plugin to get progess of build and provide better stats of output files",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "del-cli --dot=true \"./lib/**/*\"",
    "cleanTest": "del-cli --dot=true \"./dist/**/*\"",
    "build": "npm run build.compile && npm run build.types",
    "build.compile": "cross-env NODE_ENV=production babel src -d lib --verbose --extensions .ts --ignore */**/*.d.ts,**/**/*.test.ts --source-maps",
    "build.types": "cross-env NODE_ENV=production tsc --project tsconfig.types.json",
    "test": "webpack --config ./example/webpack.prod.js --mode production",
    "prepublish": "npm run build"
  },
  "author": "Abhishek Saha",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajaxtown/webpack-stats-progress.git"
  },
  "keywords": [
    "webpack",
    "typescript",
    "plugin",
    "progress",
    "stats",
    "bar"
  ],
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/node": "^14.0.4",
    "@types/progress": "^2.0.3",
    "@types/webpack": "^4.41.13",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "del-cli": "^3.0.1",
    "lodash-es": "^4.17.15",
    "mini-css-extract-plugin": "^0.9.0",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-stats-progress": "^0.0.2"
  },
  "peerDependencies": {
    "webpack": "*"
  },
  "dependencies": {
    "filesize": "^6.1.0",
    "gzip-size": "^5.1.1",
    "progress": "^2.0.3",
    "recursive-readdir": "^2.2.2"
  }
}
